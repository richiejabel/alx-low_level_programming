#include "main.h"
#include <unistd.h>
/**
 * main -Entry point
 * _putchar - prints characters
 * print_alphabet - prints the alphabet in lowercase
 */

void print_alphabet(void)
{
	char letter;

	for (letter = 'a'; letter <= 'z'; letter++)
		_putchar(letter);

	_putchar('\n');
}
int _putchar(char c)
{
	return (write(1, &c, 1));
}
